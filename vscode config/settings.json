{
  "terminal.integrated.defaultProfile.windows": "Git Bash",
"security.workspace.trust.untrustedFiles": "open",
  "liveServer.settings.host": "localhost",
  "editor.linkedEditing": true,
  "workbench.iconTheme": "icons",
  "rapidapi.terminalLink.enabled": false,
  "liveServer.settings.donotVerifyTags": true,
  "editor.fontFamily": "Waree",
  "editor.fontLigatures": true,
  "editor.insertSpaces": true,
  "editor.tabSize": 4,
  "editor.detectIndentation": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.wordWrap": "bounded",
  "emmet.preferences": {},
  "tabnine.experimentalAutoImports": true,
  "explorer.confirmDragAndDrop": false,
"git.openRepositoryInParentFolders": "never",
  "cSpell.languageSettings": [],
"workbench.startupEditor": "none",
  "editor.fontVariations": false,
  "emmet.includeLanguages": {
    "vue": "html",
    "vue-html": "html",
    "javascript": "javascriptreact",
    "django-html": "html",
  },
  "workbench.list.smoothScrolling": true,
  "editor.smoothScrolling": true,
  "editor.scrollBeyondLastLine": false,
  "editor.semanticHighlighting.enabled": true,
  "terminal.integrated.smoothScrolling": true,
  "volar.inlayHints.eventArgumentInInlineHandlers": false,
//   Developer: Inspect Editor Tokens and Scope
  "editor.tokenColorCustomizations": {
    "[Monokai Dimmed]": {
        "textMateRules": [
            {
              "scope": "emphasis",
              "settings": {
                "fontStyle": "italic"
              }
            },
            {
              "scope": "strong",
              "settings": {
                "fontStyle": "bold"
              }
            },
            {
              "scope": "entity.other.attribute-name.html",
              "settings": {
                "fontStyle": "italic"
              }
            },
            {
              "scope": "markup.underline",
              "settings": {
                "fontStyle": "underline"
              }
            },
            {
              "scope": "markup.bold",
              "settings": {
                "fontStyle": "bold"
              }
            },
            {
              "scope": "markup.heading",
              "settings": {
                "fontStyle": "bold"
              }
            },
            {
              "scope": "markup.italic",
              "settings": {
                "fontStyle": "italic"
              }
            },
            {
              "scope": "storage.type",
              "settings": {
                "fontStyle": "italic"
              }
            },
            {
              "scope": "storage.modifier",
              "settings": {
                "fontStyle": "italic"
              }
            },
            {
              "name": "String interpolation",
              "scope": [
                "punctuation.definition.template-expression.begin",
                "punctuation.definition.template-expression.end",
                "punctuation.section.embedded"
              ],
              "settings": {
                "fontStyle": "italic"
              }
            },
            {
              "scope": "keyword.control",
              "settings": {
                "fontStyle": "italic"
              }
            },
            {
              "scope": [
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.cast",
                "keyword.operator.sizeof",
                "keyword.operator.logical.python"
              ],
              "settings": {
                "fontStyle": "italic"
              }
            },
            {
              "name": "this.self",
              "scope": "variable.language",
              "settings": {
                "fontStyle": "italic",
                "foreground": "#fa426187"
              }
            },
            {
              "name": "@Decorator",
              "scope": ["meta.decorator punctuation.decorator"],
              "settings": {
                "fontStyle": "italic"
              }
            },

            // arguments to a function
            {
              "scope": ["variable.parameter"],
              "settings": {
                "foreground": "#078448be",
                "fontStyle": "italic",
              }
            }
          ]
    },
    // OK
    "comments": {
        "foreground": "#525252",
        // "fontStyle": "italic"
    },
    // "variables": {
    //     "foreground": "#00bd78",
    //     "fontStyle": ""
    // },
    // "types": {
    //     "foreground": "#7d0000",
    //     "fontStyle": ""
    // },
    // OK
    "strings": {
        // 100% OK
        "foreground": "#9AA83A",
        "fontStyle": ""
    },
    "functions": {
        // "foreground": "#ac46c5",
        "foreground": "#d97706",
        // bold - 50%-50%
        "fontStyle": "underline"
    },
    "keywords": {
        // "foreground": "#5073ff",
        // italic for keywords - 100% OK
        "fontStyle": "italic"
    },
    "numbers": {
        // kinda OK
        "foreground": "#3bac9b",
    //     "fontStyle": ""
    }
  },
"terminal.integrated.env.windows": {},
  "workbench.settings.useSplitJSON": true,
  "json.schemas": [],
  "workbench.colorCustomizations": {
    "tab.activeBackground": "#c7000090",
    "editorCursor.foreground": "#fff"
  },
  "workbench.sideBar.location": "right",
  "explorer.openEditors.visible": 0,
  "cSpell.allowedSchemas": [
    "file",
    "gist",
    "sftp",
    "untitled",
    "vscode-notebook-cell",
    "vscode-scm",
    "vscode-userdata",
    "html"
  ],
"git.confirmSync": false,
  "git.enableSmartCommit": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[jsonc]": {
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
"javascript.inlayHints.variableTypes.suppressWhenTypeMatchesName": false,
  "githubPullRequests.createOnPublishBranch": "never",
  "git.openAfterClone": "always",
  "liveServer.settings.donotShowInfoMsg": true,
  "ecsstractor_port.bem_nesting": true,
"ecsstractor_port.attributes": "className",
  "editor.minimap.enabled": false,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "javascript.inlayHints.variableTypes.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterTypes.enabled": true,
"editor.inlayHints.padding": true,
  "workbench.colorTheme": "Monokai Dimmed",
  "editor.cursorWidth": 3,
"editor.cursorBlinking": "phase",
"prettier.tabWidth": 4,
"prettier.singleAttributePerLine": true,
"prettier.trailingComma": "all",
"prettier.useTabs": true,
"prettier.vueIndentScriptAndStyle": true,
