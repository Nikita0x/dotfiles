  "diffEditor.ignoreTrimWhitespace": false,
  
  //Custom syntax highlighter
  // background: #1E1E1E
  "editor.tokenColorCustomizations": {
      "functions": {
          "fontStyle": "underline",
          "foreground": "#d97706"
      },
          "numbers": {
          "foreground": "#408080",
      },
          "keywords": {
          "foreground": "#9872A2",
          "fontStyle": "italic"
      },
          "strings": {
          "foreground": "#9AA83A",
          "fontStyle": ""
      },
      "comments": "#525252",
  
      "types": "#b7ff00ce",
      // "variables": "#FF0000",
  
      // Javascript / Typescript
      "textMateRules": [
          // colors for true / false
          {
              "scope": ["constant.language.boolean.false", "constant.language.boolean.true", "constant.language.json.comments"],
              "settings": {
                  "foreground": "#408080"
              }
          },
          // this keyword
          {
              "scope": ["variable.language.this"],
              "settings": {
                  "foreground": "#C7444A"
              }
          },
          // arguments/parameters to a function
          {
              "scope": "variable.parameter",
              "settings": {
                  "foreground": "#078448be",
                  "fontStyle": "italic",
              }
          },
          // other
          {
              "scope": ["punctuation.accessor", "punctuation.terminator.statement", "keyword.operator.new", "keyword.operator.ternary", "punctuation.definition.parameters.begin","punctuation.definition.parameters.end", "punctuation.definition.typeparameters.begin", "punctuation.definition.typeparameters.end", "meta.brace.round", "meta.type.annotation", "punctuation.definition.block", "keyword.operator.type.annotation", "punctuation.separator.comma", "punctuation.separator.parameter", "keyword.operator.assignment", "meta.brace.square", "punctuation.definition.interpolation.begin.html.vue", "punctuation.definition.interpolation.end.html.vue"],
              "settings": {
                  "foreground": "#676867"
              }
          },
          {
              "scope": ["support.type",],
              "settings": {
                  // "foreground": "#676867"
                  "foreground": "#4EC9B0"
              }
          },
          // escape character
          {
              "scope": "constant.character.escape",
              "settings": {
                  "foreground": "#8080FF"
              }
          },
          // async keyword
          {
              "scope": "storage.modifier.async",
              "settings": {
                  "foreground": "#9872A2",
              }
          },
          // optional ? in func signature
          {
              "scope": "keyword.operator.optional",
              "settings": {
                  "foreground": "#676867",
                  // "fontStyle": "strikethrough",
              }
          },
          // arrow function
          {
              "scope": "storage.type.function.arrow",
              "settings": {
                  "foreground": "#d97706",
                  "fontStyle": "underline",
              }
          },
          // arithmetic operators
          {
              "scope": "keyword.operator.arithmetic",
              "settings": {
                  "foreground": "#F92672",
              }
          },
          // html attributes color
          {
              "scope": ["entity.other.attribute-name.html.vue", "entity.other.attribute-name.html"],
              "settings": {
                  "foreground": "#D0B344",
              }
          },
          // json settings
          {
              "scope": ["punctuation.definition.dictionary.begin.json.comments", "punctuation.definition.dictionary.end.json.comments", "punctuation.definition.array.begin.json.comments", "punctuation.definition.array.end.json.comments", "punctuation.separator.dictionary.key-value.json.comments"],
              "settings": {
                  "foreground": "#676867"
              }
          },
          // CSS settings
          {
              "scope": ["entity.other.attribute-name.class.css", "meta.property-list.css", "entity.name.tag.css"],
              "settings": {
                  "foreground": "#D0B344"
              }
          },
          {
              "scope": ["punctuation.section.property-list.begin.bracket.curly.css", "punctuation.section.property-list.end.bracket.curly.css", "punctuation.section.property-list.end.bracket.curly.css", "punctuation.section.function.begin.bracket.round.css", "punctuation.section.function.end.bracket.round.css", "keyword.other.unit"],
              "settings": {
                  "foreground": "#676867"
              }
          },
          {
              "scope": ["support.constant.property-value.css"],
              "settings": {
                  "foreground": "#C7444A"
              }
          },
          {
              "scope": ["constant.other.color.rgb-value.hex.css"],
              "settings": {
                  "foreground": "#8080FF"
              }
          },
      ]
    },
  }